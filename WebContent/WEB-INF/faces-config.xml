<?xml version="1.0" encoding="UTF-8"?>
<faces-config 			 
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>main.com.m3c.gp.beans.Login</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registerBean</managed-bean-name>
		<managed-bean-class>main.com.m3c.gp.beans.Register</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
<managed-bean>
	<managed-bean-name>graphBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Graph</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>orderBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Order</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>portfolioBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Portfolio</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>personaliseBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Personalise</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>depositBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Deposit</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>logoutBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.Logout</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>changePassBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.ChangePass</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>deleteAccountBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.DeleteAccount</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>
<managed-bean>
	<managed-bean-name>marketDataBean</managed-bean-name>
	<managed-bean-class>main.com.m3c.gp.beans.MarketData</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>			   							
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>dashboard</from-outcome>
			<to-view-id>/dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>register.xhtml</display-name>
		<from-view-id>/register.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>relogin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>dashboard.xhtml</display-name>
		<from-view-id>/dashboard.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>graph</from-outcome>
			<to-view-id>/view/test.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
